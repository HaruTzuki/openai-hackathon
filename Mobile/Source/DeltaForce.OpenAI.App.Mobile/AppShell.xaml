<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DeltaForce.OpenAI.App.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DeltaForce.OpenAI.App.Mobile"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Take a Picture"
            Icon="network.svg">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="History"
            Icon="network.svg">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Settings"
            Icon="network.svg">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Login"
            Icon="network.svg">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
    </TabBar>

</Shell>
